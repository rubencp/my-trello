<p id="notice"><%= notice %></p>

<h1>Listing Sa Requests</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Topic</th>
      <th>Status</th>
      <th>Decision</th>
      <th>Applicant</th>
      <th>Sa incharge</th>
      <th>Impacted projects</th>
      <th>Sub projects</th>
      <th>Mechanical interfaces</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sa_requests.each do |sa_request| %>
      <tr>
        <td><%= sa_request.title %></td>
        <td><%= sa_request.topic %></td>
        <td><%= sa_request.status %></td>
        <td><%= sa_request.decision %></td>
        <td><%= sa_request.applicant %></td>
        <td><%= sa_request.sa_incharge %></td>
        <td><%= sa_request.impacted_projects %></td>
        <td><%= sa_request.sub_projects %></td>
        <td><%= sa_request.mechanical_interfaces %></td>
        <td><%= link_to 'Show', sa_request %></td>
        <td><%= link_to 'Edit', edit_sa_request_path(sa_request) %></td>
        <td><%= link_to 'Destroy', sa_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sa request', new_sa_request_path %>
